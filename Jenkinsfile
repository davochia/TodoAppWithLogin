pipeline{

    agent any
  
    stages {
  
        stage("build"){
            
            steps{
                
                echo 'Building application'
                echo 'start application build'

      
            }
        }
        
        stage("test"){
            
            steps{
                
                echo 'Testing application'
            }
        }
        
        stage("deploy"){
            
            steps{
                
                echo 'Deploying application'
            }
        }
        
        stage("cleanup"){
            
            steps{
                
                echo 'Clean up application'
            }
        }
    }

}
